{% form_theme form '@PrestaShop/Admin/TwigTemplateForm/prestashop_ui_kit.html.twig' %}

{{ form_start(form) }}
<div class="card">
    <h3 class="card-header">
        <i class="material-icons">schedule</i>
        {{ 'PayPal Carrier Tracking'|trans({}, 'Modules.Paypaltracking.Admin') }}
    </h3>
    <div class="card-block row">
        <div class="card-text">
            {{ form_widget(form) }}
        </div>
    </div>
    <div class="card-footer">
        <a href="{{ pathWithBackUrl('admin_paypal_tracking') }}" class="btn btn-outline-secondary">
            {{ 'Cancel'|trans({}, 'Admin.Actions') }}
        </a>
        <button class="btn btn-primary float-right">
            {{ 'Save'|trans({}, 'Admin.Actions') }}
        </button>
    </div>
</div>
{{ form_end(form) }}
