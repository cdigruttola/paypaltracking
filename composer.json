{
  "name": "cdigruttola/paypaltracking",
  "description": "PayPal Tracking",
  "authors": [
    {
      "name": "cdigruttola",
      "email": "c.digruttola@hotmail.it"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "~5.3",
    "prestashop/module-lib-service-container": "^2.0",
    "symfony/dependency-injection": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "cdigruttola\\Module\\PaypalTracking\\": "src/"
    },
    "classmap": [
      "paypaltracking.php",
      "classes"
    ],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false,
    "platform": {
      "php": "5.6"
    }
  },
  "type": "prestashop-module",
  "author": "cdigruttola"
}
